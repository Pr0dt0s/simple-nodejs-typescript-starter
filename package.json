{
  "name": "Node-Example",
  "version": "0.0.1",
  "description": "Node Project starting point",
  "main": "server.js",
  "scripts": {
    "clean": "rm -Rf _build",
    "lint": "tslint 'src/**/*.ts' 'server*.ts'",
    "compile": "tsc",
    "coverage": "npm run cover --coverage",
    "cover": "node_modules/istanbul/lib/cli.js test node_modules/tape/bin/tape ./_build/**/*.test.js | tap-spec",
    "server:dev": "concurrently --kill-others 'tsc -w' 'cross-env NODE_PATH=. NODE_ENV=development nodemon --harmony ./_build/server.js'",
    "dev": "npm run clean && npm run compile && npm run server:dev",
    "test": "ts-node node_modules/tape/bin/tape ./**/*.test.ts | tap-spec",
    "test:watch": "ts-node node_modules/tape-watch/bin/tape-watch ./src/**/*.test.ts -r typescript -p tap-spec"
  },
  "keywords": [],
  "author": "Hicham El Hammouchi<hicham.elhammouchi@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^2.1.0",
    "eslint": "^2.10.2",
    "istanbul": "^0.4.3",
    "nodemon": "^1.9.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tape-watch": "^2.1.0",
    "ts-node": "^0.7.3",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^1.0.8",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "helmet": "^2.1.0",
    "i18next": "^3.1.0",
    "i18next-express-middleware": "^1.0.1",
    "morgan": "^1.7.0",
    "nconf": "^0.8.4",
    "nunjucks": "^2.4.2",
    "serve-favicon": "^2.3.0",
    "winston": "^2.2.0"
  }
}
